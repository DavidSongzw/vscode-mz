{
    "name": "mz",
    "displayName": "妹子陪伴你",
    "description": "写代码不再孤单",
    "version": "0.1.0",
    "publisher": "szw",
    "icon": "logo.jpg",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "提醒",
        "鼓励",
        "陪伴"
    ],
    "bugs": {
        "url": "https://github.com/DavidSongzw/vscode-mz/issues"
    },
    "homepage": "https://github.com/DavidSongzw/vscode-mz/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/DavidSongzw/vscode-mz.git"
    },
    "activationEvents": [
        "onCommand:mz.showReminderView",
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "mz.showReminderView",
                "title": "打开提醒页面",
                "category": "mz"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "妹子",
            "properties": {
                "mz.reminderViewIntervalInMinutes": {
                    "type": "number",
                    "default": 60,
                    "description": "展示提醒页面的时间间隔（分钟）"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^14.11.2",
        "typescript": "^4.0.3",
        "vsce": "1.103.1",
        "vscode": "^1.1.21"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
    "dependencies": {
        "undici": "5.28.4"
    }
}